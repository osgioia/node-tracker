{
  "name": "node-tracker",
  "version": "1.0.0",
  "description": "An semi-private bittorrent tracker made with nodejs",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "nodemon index.js",
    "build:dev": "npx prisma generate  --schema=./prisma/schema.prisma && npx prisma migrate dev",
    "build": "npx prisma generate  --schema=./prisma/schema.prisma && npx prisma migrate deploy",
    "seed": "node prisma/seed.js",
    "test": "NODE_OPTIONS=\"--experimental-vm-modules\" jest --detectOpenHandles",
    "test:watch": "NODE_OPTIONS=\"--experimental-vm-modules\" jest --watch",
    "test:coverage": "NODE_OPTIONS=\"--experimental-vm-modules\" jest --coverage",
    "test:verbose": "NODE_OPTIONS=\"--experimental-vm-modules\" jest --verbose",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:check": "eslint . --max-warnings 0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/osgioia/node-tracker"
  },
  "author": "osgioia",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/osgioia/node-tracker/issues"
  },
  "homepage": "https://github.com/osgioia/node-tracker#readme",
  "dependencies": {
    "@prisma/client": "^6.2.1",
    "bcrypt": "^6.0.0",
    "bittorrent-tracker": "^11.2.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "express-rate-limiter": "^1.3.1",
    "express-slow-down": "^3.0.0",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "ip-address": "^10.0.1",
    "jsonwebtoken": "^9.0.2",
    "magnet-uri": "^7.0.7",
    "morgan": "^1.10.1",
    "node-fetch": "^3.3.2",
    "prom-client": "^15.1.3",
    "redis": "^5.8.2",
    "rate-limit-redis": "^4.2.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "jest": "^30.0.5",
    "nodemon": "^3.1.10",
    "prisma": "^6.2.1",
    "supertest": "^7.1.4"
  }
}
